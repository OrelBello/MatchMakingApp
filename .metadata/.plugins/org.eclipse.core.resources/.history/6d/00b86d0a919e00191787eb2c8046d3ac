public class Date {
Date(int day, int month, int year) 
{
	if ((day > 0 )&&( day < 32))
			{
this.day = day;
			}
	if ((month > 0 )&&( month < 13))
			{
		this.month = month;
			}
	if ((year > 1920 )&&( year < 2002))
			{
		this.month = month;
			}
this.year = year;
}
	int day, month, year;
	
void SetDay(int day)
{
	if ((day > 0 )&&( day < 32))
	{
this.day = day;
	}
	else 
		System.out.println("Invaild parameter!");
}

	void SetMonth(int month)
	{
		if ((month > 0 )&&( month < 13))
		{
	this.month = month;
		}
		else 
			System.out.println("Invaild parameter!");
	}
	
		void SetYear(int year)
		{
			if ((year > 1920 )&&( year < 2002))
			{
		this.year = year;
			}
			else 
				System.out.println("Age do not meet the requirments! Must be between 18-99!");
		}
		
		int GetDay()
		{
			return this.day;
		}
		
		int GetMonth()
		{
			return this.month;
		}
		
		int GetYear()
		{
			return this.year;
		}
}
